{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "extends": "airbnb",
  "plugins": [
    "jsx-ally",
    "import",
    "node",
    "promise",
    "react",
    "prettier",
    "babel",
    "jest"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "serviceworker": true,
    "worker": true
  },
  "rules": {
    "jsx-quotes": [
      "0"
    ],
    "prettier/prettier": "error"
  }
}
